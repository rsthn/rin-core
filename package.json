{
    "name": "@rsthn/rin-front",
    "version": "1.0.26",
    "description": "Rin Front-End Library",
    "main": "main.js",
    "exports": {
        ".": {
            "require": "./main.js",
            "default": "./main.mjs"
        }
    },
    "scripts": {
        "test": "echo Ok",
        "requirements": "yarn global add --ignore-optional parcel-bundler http-server",
        "deploy": "npm run build && node ./local/deploy.js",
        "commit": "npm run build && node ./local/commit.js",
        "build": "parcel build ./main.js -o rin-front.js --global rfront",
        "watch": "parcel watch ./main.js -d local -o rin-front.js --global rfront --no-hmr",
        "serve": "http-server -c-1 -p 8080 ./local"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/rsthn/rin-front.git"
    },
    "author": "RedStar Technologies",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/rsthn/rin-front/issues"
    },
    "homepage": "https://github.com/rsthn/rin-front#readme",
    "browserslist": [
        "since 2019-01"
    ],
    "files": [
        "elems/",
        "*.js",
        "LICENSE",
        "README.md"
    ],
    "dependencies": {
        "@rsthn/rin": "^2.0.21",
        "base-64": "^1.0.0",
        "node-fetch": "^2.6.1"
    }
}